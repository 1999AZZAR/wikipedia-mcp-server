# Example environment variables for Wikipedia MCP Server
# PORT number for Express server
PORT=3000
# Max entries for LRU cache (optional override)
CACHE_MAX=100
# TTL for LRU cache in ms (optional override)
CACHE_TTL=300000

# Rate limit window in milliseconds (optional override)
RATE_LIMIT_WINDOW=60000

# Max requests per rate limit window (optional override)
RATE_LIMIT_MAX=100
